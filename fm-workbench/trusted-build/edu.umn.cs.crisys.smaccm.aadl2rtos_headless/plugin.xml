<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="edu.umn.cs.crisys.smaccm.aadl2rtos_headless.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="edu.umn.cs.crisys.smaccm.aadl2rtos_headless.application"
            name="aadl2rtos trusted build">
         <property
               name="appName"
               value="aadl2rtos trusted build">
         </property>
      </product>
   </extension>

</plugin>
