        <module name="armv7m.rtos-kochab">
            <prefix>rtos_</prefix>
            <taskid_size>8</taskid_size>
            <signalset_size>8</signalset_size>
            <irqeventid_size>8</irqeventid_size>
            <tasks>
                <task>
                    <name>thread_a</name>
                    <entry>Thread_A</entry>
                    <stack_size>64</stack_size>
                    <priority>10</priority>
                </task>
                <task>
                    <name>thread_b</name>
                    <entry>Thread_B</entry>
                    <stack_size>64</stack_size>
                    <priority>8</priority>
                </task>
            </tasks>
            <mutexes>
                <mutex>
                    <name>mutex_datap_instance_1</name>
                </mutex>
            </mutexes>
            <semaphores/>
            <irq_events>
                <irq_event>
                    <name>smaccm_periodic_dispatch_thread_a</name>
                    <task>0</task>
                    <!--Task: thread_a has the 0th highest priority in the system.-->
                    <sig_set>1</sig_set>
                </irq_event>
                <irq_event>
                    <name>smaccm_periodic_dispatch_thread_b</name>
                    <task>1</task>
                    <!--Task: thread_b has the 1th highest priority in the system.-->
                    <sig_set>1</sig_set>
                </irq_event>
            </irq_events>
        </module>
